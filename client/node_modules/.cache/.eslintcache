 === 0 ? 5 : slideNumber - 1;\r\n    } else {\r\n      newSlideNumber = slideNumber === 5 ? 0 : slideNumber + 1;\r\n    }\r\n\r\n    setSlideNumber(newSlideNumber)\r\n  };\r\n\r\n  const handleClick = () => {\r\n       if (user) {\r\n        setOpenModal(true);\r\n       } else {\r\n        navigate(\"/login\");\r\n       }\r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <Navbar />\r\n      <Header type=\"list\" />\r\n      {loading ? ( \r\n        \"Loading\" \r\n      ) : (\r\n        <div className=\"hotelContainer\">\r\n        {open && (\r\n          <div className=\"slider\">\r\n            <FontAwesomeIcon\r\n              icon={faCircleXmark}\r\n              className=\"close\"\r\n              onClick={() => setOpen(false)}\r\n            />\r\n            <FontAwesomeIcon\r\n              icon={faCircleArrowLeft}\r\n              className=\"arrow\"\r\n              onClick={() => handleMove(\"l\")}\r\n            />\r\n            <div className=\"sliderWrapper\">\r\n              <img src={data.photos[slideNumber]} alt=\"\" className=\"sliderImg\" />\r\n            </div>\r\n            <FontAwesomeIcon\r\n              icon={faCircleArrowRight}\r\n              className=\"arrow\"\r\n              onClick={() => handleMove(\"r\")}\r\n            />\r\n          </div>\r\n        )}\r\n        <div className=\"hotelWrapper\">\r\n          <button className=\"bookNow\">Reserve or Book Now!</button>\r\n          <h1 className=\"hotelTitle\">{data.name}</h1>\r\n          <div className=\"hotelAddress\">\r\n            <FontAwesomeIcon icon={faLocationDot} />\r\n            <span>{data.address}</span>\r\n          </div>\r\n          <span className=\"hotelDistance\">\r\n            Excellent location â€“ {data.distance}m from center\r\n          </span>\r\n          <span className=\"hotelPriceHighlight\">\r\n            Book a stay over ${data.cheapestPrice} at this property and get a free airport taxi\r\n          </span>\r\n          <div className=\"hotelImages\">\r\n            {data.photos?.map((photo, i) => (\r\n              <div className=\"hotelImgWrapper\" key={i}>\r\n                <img\r\n                  onClick={() => handleOpen(i)}\r\n                  src={photo}\r\n                  alt=\"\"\r\n                  className=\"hotelImg\"\r\n                />\r\n              </div>\r\n            ))}\r\n          </div>\r\n          <div className=\"hotelDetails\">\r\n            <div className=\"hotelDetailsTexts\">\r\n              <h1 className=\"hotelTitle\">Best Hotel in the City</h1>\r\n              <p className=\"hotelDesc\">\r\n                {data.desc}\r\n              </p>\r\n            </div>\r\n            <div className=\"hotelDetailsPrice\">\r\n              <h1>Perfect for a {days}-night stay!</h1>\r\n              <span>\r\n                Located in the real heart of Krakow, this property has an\r\n                excellent location score of 9.8!\r\n              </span>\r\n              <h2>\r\n                <b>${ days * data.cheapestPrice * options.room }</b> ({days} nights)\r\n              </h2>\r\n              <button onClick={handleClick}>Reserve or Book Now!</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <MailList />\r\n        <Footer />\r\n      </div>)}\r\n      {openModal && <Reserve setOpen={setOpenModal} hotelId={id}/>}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Hotel;\r\n",[],"D:\\ExpressJS-ReactJS\\BOOKINGAPP\\client\\src\\pages\\list\\List.jsx",["97","98","99"],[],"D:\\ExpressJS-ReactJS\\BOOKINGAPP\\client\\src\\pages\\home\\Home.jsx",[],[],[],"D:\\ExpressJS-ReactJS\\BOOKINGAPP\\client\\src\\components\\navbar\\Navbar.jsx",[],[],"D:\\ExpressJS-ReactJS\\BOOKINGAPP\\client\\src\\components\\footer\\Footer.jsx",[],[],"D:\\ExpressJS-ReactJS\\BOOKINGAPP\\client\\src\\components\\searchItem\\SearchItem.jsx",[],[],"D:\\ExpressJS-ReactJS\\BOOKINGAPP\\client\\src\\components\\featuredProperties\\FeaturedProperties.jsx",["100"],[],